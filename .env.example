# Twilio Account SID (found in your Twilio Console)
TWILIO_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Twilio Auth Token (found in your Twilio Console)
TWILIO_TOKEN=your_twilio_auth_token

# Twilio WhatsApp Sandbox number (used for testing WhatsApp API)
# This is a shared number provided by Twilio for development and testing only
TWILIO_WHATSAPP_NUMBER=whatsapp:+14155238886

# API Key used to authorize requests to your PHP script
# Set any strong, random value here and use it in the "Authorization: Bearer {API_KEY}" header
API_KEY=your-api-key-here
